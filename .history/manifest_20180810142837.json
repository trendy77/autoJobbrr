  {
      "name": "autoEmployMe JobApplier",
      "version": "1.31",
      "options_page": "options.html",
      "description": "Time to get a job... Why waste time manually applying like a chump? Let's get Google to do the heavy lifting... Annnnnd away we go...!",
      "icons": {
        "16": "images/gear16.png",
        "32": "images/gear32.png",
        "48": "images/gear48.png",
        "128": "images/gear128.png"
      },
      "permissions": ["activeTab", "declarativeContent", "storage","contextMenus","alarms", "tabs", "bookmarks", "declarativeWebRequest", "*://*/*"],
      "background": {
          "scripts": [
              "background.js"
          ],
          "persistent": false
      },
      "content_scripts": [
        {
          "matches": ["http://*.seek.com.au/*"],
          "exclude_matches": ["*://*/*business*"],
          "js": ["content.js"]
        }],
      "page_action": {
          "default_popup": "frameless_window.html",
          "default_icon": {
              "16": "images/gear16.png",
              "32": "images/gear32.png",
              "48": "images/gear48.png",
              "128": "images/gear128.png"
          }
      },
      "commands": {
        "open-google": {
          "description": "Open a tab to seek",
          "suggested_key": { "default": "Ctrl+Shift+L" }
        },
        "_execute_browser_action": {
          "suggested_key": { "default": "Ctrl+Shift+K" }
        }
      },
     "manifest_version": 2
  }
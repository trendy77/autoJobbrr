  {
      "name": "autoEmployMe JobApplier",
      "version": "2.31",
      "options_page": "opts/options.html",
      "description": "Time to get a job... Why waste time manually applying? Annnnnd away we go...!",
      "icons": {
        "16": "images/gear16.png",
        "32": "images/gear32.png",
        "48": "images/gear48.png",
        "128": "images/gear128.png"
      },
        "permissions" : [
                      "declarativeContent", "storage","webNavigation",
                      "contextMenus", "https://docs.google.com/feeds/*", "https://www.google.com/accounts/OAuthGetRequestToken", "https://www.google.com/accounts/OAuthAuthorizeToken",
   "https://www.google.com/accounts/OAuthGetAccessToken","tabs"],
          "background": {
          "scripts": ["js/background.js","js/jquery-3.3.1.min.js","js/chrome_ex_oauthsimple.js","js/chrome_ex_oauth.js"],
          "persistent": true
      },
      "content_scripts": [
        {
        "matches": ["*://*.seek.com.au/*"],
       // "css": ["css/materialize.min.css","css/style.css"],
         "js": [ "js/content.js"]
        }
       ],
      "browser_action": {
          "default_title": "LAUNCH autoApplier",
          "default_popup": "options.html",
          "default_icon": {
              "16": "images/gear16.png",
              "32": "images/gear32.png",
              "48": "images/gear48.png",
              "128": "images/gear128.png"
          }
      },
     "manifest_version": 2
  }
